    public void dcnx(){//单词逆序
        String s=new Scanner(System.in).nextLine();
        String[] a=s.split(" ");
        for(int i=a.length-1;i>=0;i--){
            if(i!=0){
                System.out.print(a[i]+" ");
            }
            if(i==0){
                System.out.print(a[i]);
            }
        }
    }
